{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint .",
    "format": "prettier --write .",
    "sb": "storybook dev -p 6006",
    "b:sb": "storybook build",
    "pkgs:audit": " yarn audit --json > audit.json",
    "test:cv": "yarn run test --coverage --watchAll=false",
    "perf:audit": "npx lighthouse http://localhost:3000/ --output html --output-path ./performance.html",
    "pre:commit": "yarn run lint",
    "pre:push": "yarn run test:cv && yarn run build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.59.20",
    "@tanstack/react-query-devtools": "^5.59.20",
    "axios": "^1.7.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.28.0",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.13",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@chromatic-com/storybook": "^3.2.2",
    "@eslint/js": "^9.14.0",
    "@storybook/addon-essentials": "^8.4.2",
    "@storybook/addon-interactions": "^8.4.2",
    "@storybook/addon-onboarding": "^8.4.2",
    "@storybook/blocks": "^8.4.2",
    "@storybook/preset-create-react-app": "^8.4.2",
    "@storybook/react": "^8.4.2",
    "@storybook/react-webpack5": "^8.4.2",
    "@storybook/test": "^8.4.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "14.1.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^16.7.13",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "eslint": "^9.14.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-storybook": "^0.11.0",
    "globals": "^15.12.0",
    "msw": "^2.6.2",
    "prettier": "3.3.3",
    "prop-types": "^15.8.1",
    "storybook": "^8.4.2",
    "typescript": "^4.4.2",
    "typescript-eslint": "^8.13.0",
    "webpack": "^5.96.1"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "msw": {
    "workerDirectory": [
      "public"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "/node_modules/",
      "/node_modules/(?!(axios)/)"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "axios": "axios/dist/node/axios.cjs"
    },
    "coveragePathIgnorePatterns": [
      "./node_modules",
      "./.storybook",
      "./coverage",
      ".styles.(ts|tsx)$",
      ".svg.(ts|tsx)$",
      ".stories.(ts|tsx)$",
      "./public",
      "./src/hooks",
      "./src/mocks"
    ],
    "coverageThreshold": {
      "<rootDir>/src/**/*.test.@(js|jsx|ts|tsx)": {
        "lines": 50
      }
    }
  }
}
